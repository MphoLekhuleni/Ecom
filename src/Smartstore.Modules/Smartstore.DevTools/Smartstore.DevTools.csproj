<Project Sdk="Microsoft.NET.Sdk.Razor">

    <PropertyGroup>
	    <Product>Smartstore DevTools</Product>
	    <OutputPath>..\..\Smartstore.Web\Modules\Smartstore.DevTools</OutputPath>
	    <OutDir>$(OutputPath)</OutDir>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="MiniProfiler.AspNetCore.Mvc" Version="4.2.59" />
        <PackageReference Include="MiniProfiler.EntityFrameworkCore" Version="4.2.59" />
    </ItemGroup>

    <ItemGroup>
        <ProjectReference Include="..\..\Smartstore.Web\Smartstore.Web.csproj">
            <Private>False</Private>
            <CopyLocal>False</CopyLocal>
            <CopyLocalSatelliteAssemblies>False</CopyLocalSatelliteAssemblies>
        </ProjectReference>
    </ItemGroup>
    <ItemGroup>
        <ProjectReference Include="..\..\Smartstore.Build\Smartstore.Build.Analyzer\Smartstore.Build.Analyzer.csproj"  OutputItemType="Analyzer" ReferenceOutputAssembly="false"/>
    </ItemGroup>

</Project>
